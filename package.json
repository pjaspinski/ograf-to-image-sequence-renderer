{
  "name": "ograf-to-image-sequence-renderer",
  "version": "0.0.0",
  "description": "App for rendering OGraf graphics and saving them as a sequence of images.",
  "type": "module",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "devDependencies": {
    "@eslint/js": "~9.17",
    "@types/eslint__js": "~8.42",
    "@types/node": "~20",
    "@typescript-eslint/parser": "~8.19",
    "eslint-config-prettier": "~9.1",
    "eslint": "~9.17",
    "globals": "~15.14",
    "prettier": "~3.4",
    "rimraf": "~6.0",
    "ts-api-utils": "~2.0",
    "typescript-eslint": "~8.19",
    "typescript": "~5.7"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "eslint .",
    "prettier": "prettier \"src/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"src/**/*.{ts,mts}\" --config .prettierrc --check"
  },
  "dependencies": {
    "tslib": "~2.8"
  }
}